{
  "expo": {
    "name": "smart-homecare-app",
    "slug": "smart-homecare-app",
    "scheme": "smarthomecareapplication",
    "version": "0.1.5",
    "orientation": "portrait",
    "icon": "./asset/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./asset/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#d0eaff"
    },
    "ios": { "supportsTablet": true },
    "android": {
      "package": "com.shc.smarthomecareapp",
      "adaptiveIcon": {
        "foregroundImage": "./asset/adaptive-icon.png",
        "backgroundColor": "#d0eaff"
      }
    },
    "web": { "favicon": "./asset/favicon.png" },
    "plugins": [
      [
        "@react-native-seoul/kakao-login",
        { "kakaoAppKey": "ebe488acefac3ddda8c04afc669b2495" }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.8.22",
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 24
          },
          "gradleProperties": {
            "android.kotlinVersion": "1.8.22"
          }
        }
      ],
      "./plugins/fix-kotlin-version",
      "expo-secure-store",
      "expo-font"
    ],
    "extra": {
      "eas": { "projectId": "4d87156c-a8ab-40c7-bcf7-48a06ee13b79" }
    }
  }
}
